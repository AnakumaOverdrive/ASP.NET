<?xml version="1.0" encoding="utf-8" ?>
<!-- CSharpType:POJO中用;DbType:映射文件中用;DataType:数据库支持类型 -->
<configuration>
  <DataType CSharpType="Integer" DbType="INTEGER" DataType="Int" Convert="Convert.ToInt32" AutoWidth="true"/>
  <DataType CSharpType="Double" DbType="DECIMAL" DataType="Decimal" Convert="Convert.ToDouble" AutoWidth="true"/>
  <DataType CSharpType="Double" DbType="FLOAT" DataType="Float" Convert="Convert.ToDouble" AutoWidth="true"/>
  <DataType CSharpType="Double" DbType="NUMERIC" DataType="Numeric" Convert="Convert.ToDouble" AutoWidth="true"/>
  <DataType CSharpType="Double" DbType="DECIMAL" DataType="Money" Convert="Convert.ToDecimal" AutoWidth="true"/>
  <DataType CSharpType="String" DbType="CHAR" DataType="Char" Convert="Convert.ToString" AutoWidth="false"/>
  <DataType CSharpType="String" DbType="NCHAR" DataType="NChar" Convert="Convert.ToString" AutoWidth="false"/>
  <DataType CSharpType="String" DbType="VARCHAR" DataType="VarChar" Convert="Convert.ToString" AutoWidth="true"/>
  <DataType CSharpType="String" DbType="NVARCHAR" DataType="NVarChar" Convert="Convert.ToString" AutoWidth="true"/>
  <DataType CSharpType="String" DbType="VARCHAR" DataType="Text" Convert="Convert.ToString" AutoWidth="true"/>
  <DataType CSharpType="String" DbType="NVARCHAR" DataType="NText" Convert="Convert.ToString" AutoWidth="true"/>
  <DataType CSharpType="Date" DbType="DATE" DataType="Date" Convert="Convert.ToDateTime" AutoWidth="true"/>
  <DataType CSharpType="Timestamp" DbType="TIMESTAMP" DataType="DateTime" Convert="Convert.ToDateTime" AutoWidth="true"/>
  <DataType CSharpType="byte[]" DbType="VARBINARY" DataType="Timestamp" Convert="(byte[])" AutoWidth="true"/>
  <DataType CSharpType="byte[]" DbType="VARBINARY" DataType="Image" Convert="(byte[])" AutoWidth="true"/>
  <DataType CSharpType="String" DbType="NVARCHAR" DataType="UniqueIdentifier" Convert="(Guid)" AutoWidth="true"/>
  <DataType CSharpType="String" DbType="NVARCHAR" DataType="XML" Convert="Convert.ToString" AutoWidth="true"/>
</configuration>