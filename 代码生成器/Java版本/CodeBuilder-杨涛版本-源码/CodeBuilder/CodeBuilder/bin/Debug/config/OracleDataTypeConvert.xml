<?xml version="1.0" encoding="utf-8" ?>
<!-- CSharpType:POJO中用;DbType:映射文件中用;DataType:数据库支持类型 -->
<configuration>
  <DataType CSharpType="Double" DbType="DOUBLE" DataType="NUMBER" Convert="Convert.ToDouble" AutoWidth="true"/>
  <DataType CSharpType="Integer" DbType="INTEGER" DataType="INTEGER" Convert="Convert.ToInt32" AutoWidth="true"/>
  <DataType CSharpType="String" DbType="CHAR" DataType="CHAR" Convert="Convert.ToString" AutoWidth="true"/>
  <DataType CSharpType="String" DbType="NCHAR" DataType="NCHAR" Convert="Convert.ToString" AutoWidth="true"/>
  <DataType CSharpType="String" DbType="VARCHAR" DataType="VARCHAR2" Convert="Convert.ToString" AutoWidth="true"/>
  <DataType CSharpType="String" DbType="NVARCHAR" DataType="NVARCHAR2" Convert="Convert.ToString" AutoWidth="true"/>
  <DataType CSharpType="byte[]" DbType="VARBINARY" DataType="BLOB" Convert="Convert.ToByte" AutoWidth="true"/>
  <DataType CSharpType="String" DbType="CLOB" DataType="CLOB" Convert="Convert.ToString" AutoWidth="true"/>
  <DataType CSharpType="String" DbType="NCLOB" DataType="NCLOB" Convert="Convert.ToString" AutoWidth="true"/>
  <DataType CSharpType="String" DbType="CLOB" DataType="XMLTYPE" Convert="Convert.ToString" AutoWidth="true"/>
  <DataType CSharpType="String" DbType="NVARCHAR" DataType="DATE" Convert="Convert.ToDateTime" AutoWidth="true"/>
  <DataType CSharpType="Timestamp" DbType="TIMESTAMP" DataType="TIMESTAMP" Convert="Convert.ToDateTime" AutoWidth="true"/>
</configuration>